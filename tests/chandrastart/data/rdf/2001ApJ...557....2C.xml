  <rdf:Description rdf:nodeID="otCfwbPL3019">
    <adsbib:identifier>1991ApJ...378..537E</adsbib:identifier>
    <adsbib:citationFrom rdf:resource="http://ads.harvard.edu/sem/bib#6a12f037-fdd0-4db4-ba7f-db500d44c157"/>
    <adsbib:citationText>Elvis, M., Wilkes, B. J., Giommi, P., &amp; McDowell, J. 1991, ApJ, 378, 537</adsbib:citationText>
    <rdf:type rdf:resource="http://github.com/rahuldave/ontoads/owl/adsbib.owl#Citation"/>
