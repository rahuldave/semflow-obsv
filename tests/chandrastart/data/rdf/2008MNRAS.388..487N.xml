    <adsbase:title>Limits on the X-ray and optical luminosity of the progenitor of the Type Ia supernova 2007sr</adsbase:title>
    <adsbib:workIdentifier>2008MNRAS.388..487N</adsbib:workIdentifier>
    <adsbib:defaultRealizedThrough rdf:resource="http://ads.harvard.edu/sem/bib#2008MNRAS.388..487N"/>
